<%= form_for @car, html: {class: "form-inline"} do |f| %>

    <div class="form-group">
      <%= f.label :brand %>
      <%= f.text_field :brand, autocomplete: "off", class: "form-control"  %>
    </div>


    <div class="form-group" data-no-turbolink>
      <%= f.label :reg_number, 'Registration number' %>
      <%= f.text_field :reg_number %>
    </div>




    <%= f.submit "Save", class: "btn btn-primary" %>

<% end %>
